<% if user_signed_in? && current_user.likes?(@artwork) %>
	<%= link_to "Unlike", artwork_like_path(@artwork), remote: true, method: :delete %>
<% else %>
	<%= link_to "Like", artwork_like_path(@artwork), remote: true, method: :post %>
<% end %>

<%= @artwork.likes.count %>

<% @artwork.likes.each do |like| %>
	<%= image_tag like.user.profile.url, width: 20 %>
<% end %>
